{
	"name": "Equinox",
	"description": "A multiplayer top-down bullet hell",
	"version": "1.0.0.0",
	"manifest_version": 2,
	"app": {
		"launch": {
			"web_url": "http://Equinoxgame"
		}
	},
	"icons": {
		"16": "icon-16.png",
		"128": "icon-128.png",
		"256": "icon-256.png"
